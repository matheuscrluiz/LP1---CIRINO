#include <stdio.h> 

int main (void)
{
    char string[100];
    int i, count =0;

    printf("\n\nDigite uma frase: ");
    scanf("%s", string); 

    for(i=0;string[i] != '\0';i++)
        {
            if(string[i] != '\0')
                {
                    count = count + 1;
                }
        }

    printf("%d",  count);

    return 0;
}
